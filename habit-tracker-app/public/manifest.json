{
  "name": "Habit.lio",
  "description": "A React-powered habit-tracking Chrome Extension",
  "version": "1.0",
  "manifest_version": 3,
  "icons": {
    "16": "icons/Habitlio-Icon16.png",
    "32": "icons/Habitlio-Icon32.png",
    "48": "icons/Habitlio-Icon48.png",
    "128": "icons/Habitlio-Icon128.png"
  },
  "permissions": [
    "sidePanel"
  ],
  "action": {
    "default_title": "Open Habit.lio"
  },

  "side_panel": {
    "default_path": "login.html"
  },
  "background": {
    "service_worker": "background.js"
  }
}